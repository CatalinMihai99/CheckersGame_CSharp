<Window x:Class="checkers_game.View.Statistici"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:checkers_game.View"
        mc:Ignorable="d"
        Title="Statistici" Height="200" Width="800">
    <Window.Background>
        <ImageBrush ImageSource="/checkers_game;component/Resources/fundal.jpg"></ImageBrush>
    </Window.Background>
    <Grid>
        <TextBox x:Name="V" HorizontalAlignment="Left" Height="23" Margin="174,70,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="30" TextChanged="TextBox_TextChanged"/>
        <TextBox x:Name="A" HorizontalAlignment="Left" Height="23" Margin="540,70,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="30" TextChanged="TextBox_TextChanged_1"/>
        <Label Content="Castiguri Verde:" FontWeight="Bold" HorizontalAlignment="Left" Margin="52,69,0,0" VerticalAlignment="Top" Width="100"/>
        <Label Content="Castiguri Albastru:" FontWeight="Bold" HorizontalAlignment="Left" Margin="418,66,0,0" VerticalAlignment="Top" Width="117"/>
        <Button Content="Back" FontWeight="Bold" HorizontalAlignment="Left" Margin="574,129,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>

    </Grid>
</Window>
